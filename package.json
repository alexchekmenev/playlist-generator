{
  "name": "playlist-generator",
  "version": "1.0.0",
  "description": "Express.js application for generating playlists written in Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production npm run build && npm run serve",
    "serve": "NODE_ENV=production node dist/server.js",
    "serve:dev": "NODE_ENV=development ts-node ./src/server.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --coverage --verbose"
  },
  "author": "Alexander Chekmenev",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.3",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  }
}
